@model IList<PushNotificationsHandler.Models.Services.IMessageModel>

@{
    ViewBag.Title = "Esendex Push Notifications - Received Messages";
    
}

<div class="jumbotron">
    <h1>Animate My SMS!</h1>
</div>

<div class="row">
    <div class="col-md-4">
        @if (Model.Any())
        {
            <table>
                <thead>
                Message Text
                </thead>
                <tbody>
                @foreach (var message in Model)
                {
                    <tr>
                        <td>
                            @message.PrintMessage()
                        </td>
                    </tr>
                }
                </tbody>
            </table>
        }

    </div>
    <div class="col-md-4">
       
    </div>
    <div class="col-md-4">
       
    </div>
</div>